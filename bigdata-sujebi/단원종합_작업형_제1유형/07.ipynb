   "source": [
    "mean = df['Hwt'].mean()\n",
    "std = df['Hwt'].std()\n",
    "\n",
    "upper = mean + std * 1.5\n",
    "lower = mean - std * 1.5\n",
    "\n",
    "hwts = df.loc[(df['Hwt'] > upper) | (df['Hwt'] < lower)]['Hwt']\n",
    "hwts.mean()"
   ]
